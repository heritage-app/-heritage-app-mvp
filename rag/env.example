# Supabase Configuration
SUPABASE_URL=your_supabase_url
# Use either SUPABASE_KEY or SUPABASE_SERVICE_ROLE_KEY (both work)
SUPABASE_KEY=your_service_role_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key
SUPABASE_BUCKET=documents


# OpenRouter Configuration
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_MODEL=meta-llama/llama-3-8b-instruct
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Qdrant Configuration
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=your_qdrant_cloud_api_key  # Required for Qdrant Cloud, optional for local

# CORS Configuration
# Comma-separated list of allowed origins (required when allow_credentials=True)
# Cannot use "*" when credentials are enabled
CORS_ORIGINS=https://heritage.ekowlabs.space,http://localhost:3000,http://localhost:5173,http://localhost:8080

